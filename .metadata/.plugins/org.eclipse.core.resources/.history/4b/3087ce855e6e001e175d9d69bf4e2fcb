package chap99_homework.hw231019._01;
// 정규직
public class PW extends Employee{
	 private int bonus;

	    public PW(int eno, String name, int pay, int bonus) {
	        super(eno, name, pay, 1);
	        this.bonus = bonus;
	    }

	    @Override
	    public double getMonthPay() {
	        return (pay / 12.0) + (bonus / 12.0);
	    }
	}

