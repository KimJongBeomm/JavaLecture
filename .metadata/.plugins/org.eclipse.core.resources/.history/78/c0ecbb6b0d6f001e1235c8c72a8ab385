package user;

public class dsafadsf {

	public static void main(String[] args) {
		// 정수형 변수 선언과 초기화
		int age = 30;
		
		// 문자열 변수 선언과 초기화
		String name = "Jong Beom";
		
		//변수 사용
		System.out.println(age);
		System.out.println(name);
		
		// 변수 값 변경
		
		age = 31;
		name = "Ji Woo";
		
		System.out.println(age);
		System.out.println(name);
		
	}

}
