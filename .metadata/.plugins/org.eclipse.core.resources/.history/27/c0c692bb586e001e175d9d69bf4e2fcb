package chap99_homework.hw231019._01;

public abstract class Employee {
	public int eno;
	public String name;
	public int pay;
	public int type;
	// 추상클래스의 생성자는 자식 클래스에서 사용가능
	
	public Employee(int Num , String name , int pay , String workType) {
		
	}
	// 추상 메소드 선언
	public abstract double getMonthPay();
	public abstract void showEmployeeInfo();
	
	
	
	
	
	
}
