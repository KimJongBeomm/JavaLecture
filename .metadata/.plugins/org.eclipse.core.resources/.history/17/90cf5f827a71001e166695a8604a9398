package chap14_objevtarray;

public class Mathmetics {
	private String subject;
	private int studuentCnt;
	private int lectureTime;
	
	public Mathmetics() {
		
	}
	public Mathmetics(String subject, int studnetCnt ,
			int lectureTinme) {
		this.subject = subject;
		this.studuentCnt = studnetCnt;
		this.lectureTime = lectureTinme;
	}
	public String getSubject() {
		return subject;
	}
	public int getStuduentCnt() {
		return studuentCnt;
	}
	public int getLectureTime() {
		return lectureTime;
	}
	
	
}
