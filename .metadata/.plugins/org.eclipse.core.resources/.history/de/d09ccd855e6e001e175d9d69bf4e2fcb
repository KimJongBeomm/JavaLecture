package chap99_homework.hw231019._01;
// 계약직
public class TW extends Employee {
    private int hireYear;

    public TW(int eno, String name, int pay, int hireYear) {
        super(eno, name, pay, 3);
        this.hireYear = hireYear;
    }

    @Override
    public double getMonthPay() {
        return pay / 12.0;
    }
}